{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    // 禁止编译错误
    "noEmitOnError":  true,
    "noImplicitAny": true,
    "outDir": "dist"
  },
}
